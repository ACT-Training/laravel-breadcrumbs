{
    "name": "act-training/laravel-breadcrumbs",
    "description": "A simple and flexible breadcrumb package for Laravel applications",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "breadcrumbs",
        "navigation",
        "ui"
    ],
    "authors": [
        {
            "name": "ACT Training",
            "email": "info@act-training.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/view": "^12.0",
        "illuminate/routing": "^12.0",
        "livewire/flux": "^1.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "ActTraining\\LaravelBreadcrumbs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActTraining\\LaravelBreadcrumbs\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ActTraining\\LaravelBreadcrumbs\\BreadcrumbsServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "ActTraining\\LaravelBreadcrumbs\\Facades\\Breadcrumbs"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}